@model string
@{
    ViewData["Title"] = "Notification";
    Layout = "_LayoutMail";
}
@section Styles{
    <link rel="stylesheet" href="~/Portal/assets/css/login_register.css" />
}
<section class="container-form gird-container-full padding-customer">
    <div class="grid-container full mt-5">
        <div class="grid-x grid-padding-x">
            <div class="cell large-10 medium-12 center">
                <form class="form validate-form m-3">
                    <div class="text-center">
                        <h1 class="h3 mb-5 fw-normal">@Model</h1>
                        <a class="btn btn-black" style="display: inline-block !important; text-align: center; height: auto; width: 18rem; padding: 9px; " href="/signin">Back</a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>
